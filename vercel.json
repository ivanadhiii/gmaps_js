{
  "version": 2,
  "builds": [
    {
      "src": "back-end/scraper.js",
      "use": "@vercel/node"
      "config": {
        "maxDuration": 10,
        "memory": 3008
      }
    },
    {
      "src": "back-end/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "back-end/scraper",
      "dest": "back-end/scraper.js"
    },
    {
      "src": "/(.*)",
      "dest": "back-end/server.js"
    }
  ]
}
